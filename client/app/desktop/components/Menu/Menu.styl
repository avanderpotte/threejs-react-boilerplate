.pageSlider-appear
  transform: translate3d(100%, 0, 0)


.pageSlider-appear.pageSlider-appear-active
  transform: translate3d(0, 0, 0)
  transition: all 5000ms

.page
  position: absolute
  top: 0
  left: 0
  width: 100%
  height: 100%
  z-index: 1000px
